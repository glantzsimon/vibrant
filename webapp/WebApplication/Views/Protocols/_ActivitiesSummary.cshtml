@using K9.Base.WebApplication.Extensions
@using K9.DataAccessLayer.Helpers
@using K9.SharedLibrary.Authentication
@using K9.WebApplication.Constants
@model Protocol

<div class="row table-row">
    <div class="col-sm-12">
        @if (Model.Activities != null && Model.Activities.Any())
        {
            <div class="summary-info-item">@Dictionary.ClickOnAnItemForMoreInfo</div>
            for (int i = 0; i < Model.Activities?.Count; i++)
            {
                <a href="#@Model.Activities[i].Activity.GetBookMark()"><span class="summary-item">@Model.Activities[i].Activity.Name</span></a>
            }
        }
    </div>
</div>
