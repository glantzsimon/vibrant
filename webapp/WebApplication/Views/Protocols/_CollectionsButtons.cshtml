@using K9.Base.WebApplication.Helpers
@using K9.WebApplication.Helpers
@model Protocol

@if (Model.Id > 0)
{
    @Html.BootstrapActionLinkButton(Dictionary.SummaryLink, "Summary", "Protocol", new { id = Model.ExternalId }, "", EButtonClass.Success)

    @Html.BootstrapActionLinkButton(Dictionary.DuplicateProtocol, "DuplicateProtocol", "Protocols", new { id = Model.Id }, "", EButtonClass.Info)

    <hr />

    @Html.BootstrapActionLinkButton(Dictionary.EditActivities, "EditActivities", "Protocols", new { id = Model.Id }, "", EButtonClass.Primary)

    @Html.BootstrapActionLinkButton(Dictionary.EditSections, "EditSections", "Protocols", new { id = Model.Id }, "", EButtonClass.Primary)

    @Html.BootstrapActionLinkButton(Dictionary.EditProducts, "EditProducts", "Protocols", new { id = Model.Id }, "", EButtonClass.Primary)

    @Html.BootstrapActionLinkButton(Dictionary.EditProductPacks, "EditProductPacks", "Protocols", new { id = Model.Id }, "", EButtonClass.Primary)

}