@using K9.SharedLibrary.Helpers.Html
@model List<K9.WebApplication.Models.LogItem>
@{
    ViewBag.Title = Dictionary.ErrorLog;
    ViewBag.SubTitle = Dictionary.ErrorLog;
    Layout = "../Shared/_LayoutFormWide.cshtml";
}

@Html.Partial("_BetaPopup")

<div class="container main-content">
    <div class="well margin-bottom-0">
        <article>
            <div class="width-100">
                @Html.Raw(Model.ToHtmlTable())
            </div>
        </article>
    </div>
</div>

<script type="text/javascript">

    $(function () {
        customColumnDefs = [
            {
                targets: [0],
                visible: false
            },
            {
                targets: [1],
                type: "date",
                width: "15%"
            },
            {
                targets: [4],
                width: "50%"
            }
        ];
        customOrder = [[1, "desc"]];
    });

</script>
