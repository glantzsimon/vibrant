@using K9.Base.WebApplication.Helpers
@using K9.WebApplication.Helpers
@model ProductPack

@if (Model.Id > 0)
{
    @Html.BootstrapActionLinkButton(Dictionary.EditProducts, "EditProducts", "ProductPacks", new { id = Model.Id }, "", EButtonClass.Info)
    
    if (Model.Products != null && Model.Products.Any())
    {
        @Html.BootstrapActionLinkButton(Dictionary.EditProductQuantities, "EditProductQuantities", "ProductPacks", new { id = Model.Id }, "", EButtonClass.Info)
    }
}