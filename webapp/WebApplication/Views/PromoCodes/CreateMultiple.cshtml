@using K9.Base.WebApplication.Helpers
@model PromoCode
@{
    Layout = "../Shared/_LayoutFormSmall.cshtml";
}
@using (Html.BeginForm("CreateMultiple", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    using (Html.BeginBootstrapForm())
    {
        @Html.BootstrapEditorFor(e => e.NumberToCreate)

        @Html.Partial("_EditFields")
        <hr />
        @Html.BootstrapButton(K9.Base.Globalisation.Dictionary.Create, EButtonType.Submit, "fa-save")
        @Html.BootstrapBackToListButton()
    }
}